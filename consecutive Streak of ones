function consecutiveStreak(inpArr) {
    let maxStreak = 0;
    let currentStreak = 0;
   for (let i=0; i<inpArr.length; i++) {
         inpArr[i] == 1 ? currentStreak++ : currentStreak = 0
       if (currentStreak > maxStreak) {
            maxStreak = currentStreak;}
    }
    return maxStreak;
}

